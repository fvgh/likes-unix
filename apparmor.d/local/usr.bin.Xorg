# Only local basic features are supported
deny /etc/nsswitch.conf r,

# Xorg seems not to require start-up information of kernel and processes.
# It is requested for processes triggered at session start.
deny @{PROC}/cmdline r,
deny @{PROC}/*/cmdline r,

